{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "ASTRA_DB_APPLICATION_TOKEN": "@astra_db_token",
    "ASTRA_DB_API_ENDPOINT": "@astra_db_endpoint"
  }
}